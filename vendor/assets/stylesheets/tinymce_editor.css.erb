/* Reset */

.defaultSkin table, .defaultSkin tbody, .defaultSkin a, .defaultSkin img, .defaultSkin tr, .defaultSkin div, .defaultSkin td, .defaultSkin iframe, .defaultSkin span, .defaultSkin *, .defaultSkin .mceText {border:0; margin:0; padding:0; background:transparent; white-space:nowrap; text-decoration:none; font-weight:normal; cursor:default; color:#000; vertical-align:baseline; width:auto; border-collapse:separate; text-align:left}
.defaultSkin a:hover, .defaultSkin a:link, .defaultSkin a:visited, .defaultSkin a:active {text-decoration:none; font-weight:normal; cursor:default; color:#000}
.#text-toolbar table td {vertical-align:middle}

/* Containers */
#text-toolbar table {direction:ltr;background:transparent}
#text-toolbar iframe {display:block;}
#text-toolbar .mceToolbar {height:26px}
#text-toolbar .mceLeft {text-align:left}
#text-toolbar .mceRight {text-align:right}

/* External */
#text-toolbar .mceExternalToolbar {position:absolute; border:1px solid #CCC; border-bottom:0; top:0px !important;}
#text-toolbar .mceExternalToolbar td.mceToolbar {padding-right:13px;}
#text-toolbar .mceExternalClose {position:absolute; top:3px; right:3px; width:7px; height:7px; background:url(<%= asset_path ('editor_icons.gif') %>) -820px 0}

/* Layout */
#tinymce { margin:0 !important;}
#text-toolbar table.mceLayout {width:100% !important; height:100% !important;}
#text-toolbar table.mceLayout {border:0; border-left:1px solid #CCC; border-right:1px solid #CCC}
#text-toolbar table.mceLayout tr.mceFirst td {border-top:1px solid #CCC}
#text-toolbar table.mceLayout tr.mceLast td {border-bottom:1px solid #CCC}
#text-toolbar table.mceToolbar, #text-toolbar tr.mceFirst .mceToolbar tr td, #text-toolbar tr.mceLast .mceToolbar tr td {border:0; margin:0; padding:0;}
#text-toolbar td.mceToolbar {background:#F0F0EE; padding-top:1px; vertical-align:top}
#text-toolbar .mceIframeContainer {border-top:1px solid #CCC; border-bottom:1px solid #CCC; height:100% !important;}
#text-toolbar .mceStatusbar {background:#F0F0EE; font-family:'MS Sans Serif',sans-serif,Verdana,Arial; font-size:9pt; line-height:16px; overflow:visible; color:#000; display:block; height:20px}
#text-toolbar .mceStatusbar div {float:left; margin:2px}
#text-toolbar .mceStatusbar a.mceResize {display:block; float:right; background:url(<%= asset_path ('editor_icons.gif') %>) -800px 0; width:20px; height:20px; cursor:se-resize; outline:0}
#text-toolbar .mceStatusbar a:hover {text-decoration:underline}
#text-toolbar table.mceToolbar {margin-left:3px}
#text-toolbar span.mceIcon, #text-toolbar img.mceIcon {display:block; width:20px; height:20px}
#text-toolbar .mceIcon {background:url(<%= asset_path ('editor_icons.gif') %>) no-repeat 20px 20px}
#text-toolbar td.mceCenter {text-align:center;}
#text-toolbar td.mceCenter table {margin:0 auto; text-align:left;}
#text-toolbar td.mceRight table {margin:0 0 0 auto;}

/* Button */
#text-toolbar .mceButton {display:block; border:1px solid #F0F0EE; width:20px; height:20px; margin-right:1px}
#text-toolbar a.mceButtonEnabled:hover {border:1px solid #0A246A; background-color:#B2BBD0}
#text-toolbar a.mceButtonActive, #text-toolbar a.mceButtonSelected {border:1px solid #0A246A; background-color:#C2CBE0}
#text-toolbar .mceButtonDisabled .mceIcon {opacity:0.3; -ms-filter:'alpha(opacity=30)'; filter:alpha(opacity=30)}
#text-toolbar .mceButtonLabeled {width:auto}
#text-toolbar .mceButtonLabeled span.mceIcon {float:left}
#text-toolbar span.mceButtonLabel {display:block; font-size:10px; padding:4px 6px 0 22px; font-family:Tahoma,Verdana,Arial,Helvetica}
#text-toolbar .mceButtonDisabled .mceButtonLabel {color:#888}

/* Separator */
#text-toolbar .mceSeparator {display:block; background:url(<%= asset_path ('editor_icons.gif') %>) -180px 0; width:2px; height:20px; margin:2px 2px 0 4px}

/* ListBox */
#text-toolbar .mceListBox, #text-toolbar .mceListBox a {display:block}
#text-toolbar .mceListBox .mceText {padding-left:4px; width:70px; text-align:left; border:1px solid #CCC; border-right:0; background:#FFF; font-family:Tahoma,Verdana,Arial,Helvetica; font-size:11px; height:20px; line-height:20px; overflow:hidden}
#text-toolbar .mceListBox .mceOpen {width:9px; height:20px; background:url(<%= asset_path ('editor_icons.gif') %>) -741px 0; margin-right:2px; border:1px solid #CCC;}
#text-toolbar table.mceListBoxEnabled:hover .mceText, #text-toolbar .mceListBoxHover .mceText, #text-toolbar .mceListBoxSelected .mceText {border:1px solid #A2ABC0; border-right:0; background:#FFF}
#text-toolbar table.mceListBoxEnabled:hover .mceOpen, #text-toolbar .mceListBoxHover .mceOpen, #text-toolbar .mceListBoxSelected .mceOpen {background-color:#FFF; border:1px solid #A2ABC0}
#text-toolbar .mceListBoxDisabled a.mceText {color:gray; background-color:transparent;}
#text-toolbar .mceListBoxMenu {overflow:auto; overflow-x:hidden}
#text-toolbar .mceOldBoxModel .mceListBox .mceText {height:22px}
#text-toolbar .mceOldBoxModel .mceListBox .mceOpen {width:11px; height:22px;}
#text-toolbar select.mceNativeListBox {font-family:'MS Sans Serif',sans-serif,Verdana,Arial; font-size:7pt; background:#F0F0EE; border:1px solid gray; margin-right:2px;}

/* SplitButton */
#text-toolbar .mceSplitButton {width:32px; height:20px; direction:ltr}
#text-toolbar .mceSplitButton a, #text-toolbar .mceSplitButton span {height:20px; display:block}
#text-toolbar .mceSplitButton a.mceAction {width:20px; border:1px solid #F0F0EE; border-right:0;}
#text-toolbar .mceSplitButton span.mceAction {width:20px; background-image:url(<%= asset_path ('editor_icons.gif') %>);}
#text-toolbar .mceSplitButton a.mceOpen {width:9px; background:url(<%= asset_path ('editor_icons.gif') %>) -741px 0; border:1px solid #F0F0EE;}
#text-toolbar .mceSplitButton span.mceOpen {display:none}
#text-toolbar table.mceSplitButtonEnabled:hover a.mceAction, #text-toolbar .mceSplitButtonHover a.mceAction, #text-toolbar .mceSplitButtonSelected a.mceAction {border:1px solid #0A246A; border-right:0; background-color:#B2BBD0}
#text-toolbar table.mceSplitButtonEnabled:hover a.mceOpen, #text-toolbar .mceSplitButtonHover a.mceOpen, #text-toolbar .mceSplitButtonSelected a.mceOpen {background-color:#B2BBD0; border:1px solid #0A246A;}
#text-toolbar .mceSplitButtonDisabled .mceAction, #text-toolbar .mceSplitButtonDisabled a.mceOpen {opacity:0.3; -ms-filter:'alpha(opacity=30)'; filter:alpha(opacity=30)}
#text-toolbar .mceSplitButtonActive a.mceAction {border:1px solid #0A246A; background-color:#C2CBE0}
#text-toolbar .mceSplitButtonActive a.mceOpen {border-left:0;}

/* ColorSplitButton */
.defaultSkin div.mceColorSplitMenu table {background:#FFF; border:1px solid gray}
.defaultSkin .mceColorSplitMenu td {padding:2px}
.defaultSkin .mceColorSplitMenu a {display:block; width:9px; height:9px; overflow:hidden; border:1px solid #808080}
.defaultSkin .mceColorSplitMenu td.mceMoreColors {padding:1px 3px 1px 1px}
.defaultSkin .mceColorSplitMenu a.mceMoreColors {width:100%; height:auto; text-align:center; font-family:Tahoma,Verdana,Arial,Helvetica; font-size:11px; line-height:20px; border:1px solid #FFF}
.defaultSkin .mceColorSplitMenu a.mceMoreColors:hover {border:1px solid #0A246A; background-color:#B6BDD2}
.defaultSkin a.mceMoreColors:hover {border:1px solid #0A246A}
.defaultSkin .mceColorPreview {margin-left:2px; width:16px; height:4px; overflow:hidden; background:#9a9b9a}
.defaultSkin .mce_forecolor span.mceAction, #text-toolbar .mce_backcolor span.mceAction {overflow:hidden; height:16px}

/* Menu */
.defaultSkin .mceMenu {position:absolute; left:0; top:0; z-index:1000; border:1px solid #D4D0C8; direction:ltr}
.defaultSkin .mceNoIcons span.mceIcon {width:0;}
.defaultSkin .mceNoIcons a .mceText {padding-left:10px}
.defaultSkin .mceMenu table {background:#FFF}
.defaultSkin .mceMenu a, #text-toolbar .mceMenu span, #text-toolbar .mceMenu {display:block}
.defaultSkin .mceMenu td {height:20px}
.defaultSkin .mceMenu a {position:relative;padding:3px 0 4px 0}
.defaultSkin .mceMenu .mceText {position:relative; display:block; font-family:Tahoma,Verdana,Arial,Helvetica; color:#000; cursor:default; margin:0; padding:0 25px 0 25px; display:block}
.defaultSkin .mceMenu span.mceText, #text-toolbar .mceMenu .mcePreview {font-size:11px}
.defaultSkin .mceMenu pre.mceText {font-family:Monospace}
.defaultSkin .mceMenu .mceIcon {position:absolute; top:0; left:0; width:22px;}
.defaultSkin .mceMenu .mceMenuItemEnabled a:hover, .defaultSkin .mceMenu .mceMenuItemActive {background-color:#dbecf3}
.defaultSkin td.mceMenuItemSeparator {background:#DDD; height:1px}
.defaultSkin .mceMenuItemTitle a {border:0; background:#EEE; border-bottom:1px solid #DDD}
.defaultSkin .mceMenuItemTitle span.mceText {color:#000; font-weight:bold; padding-left:4px}
.defaultSkin .mceMenuItemDisabled .mceText {color:#888}
.defaultSkin .mceMenuItemSelected .mceIcon {background:url(img/menu_check.gif)}
.defaultSkin .mceNoIcons .mceMenuItemSelected a {background:url(img/menu_arrow.gif) no-repeat -6px center}
.defaultSkin .mceMenu span.mceMenuLine {display:none}
.defaultSkin .mceMenuItemSub a {background:url(img/menu_arrow.gif) no-repeat top right;}
.defaultSkin .mceMenuItem td, #text-toolbar .mceMenuItem th {line-height: normal}

/* Progress,Resize */
#text-toolbar .mceBlocker {position:absolute; left:0; top:0; z-index:1000; opacity:0.5; -ms-filter:'alpha(opacity=50)'; filter:alpha(opacity=50); background:#FFF}
#text-toolbar .mceProgress {position:absolute; left:0; top:0; z-index:1001; background:url(img/progress.gif) no-repeat; width:32px; height:32px; margin:-16px 0 0 -16px}

/* Rtl */
.mceRtl .mceListBox .mceText {text-align: right; padding: 0 4px 0 0}
.mceRtl .mceMenuItem .mceText {text-align: right}

/* Formats */
#text-toolbar .mce_formatPreview a {font-size:10px}
#text-toolbar .mce_p span.mceText {}
#text-toolbar .mce_address span.mceText {font-style:italic}
#text-toolbar .mce_pre span.mceText {font-family:monospace}
#text-toolbar .mce_h1 span.mceText {font-weight:bolder; font-size: 2em}
#text-toolbar .mce_h2 span.mceText {font-weight:bolder; font-size: 1.5em}
#text-toolbar .mce_h3 span.mceText {font-weight:bolder; font-size: 1.17em}
#text-toolbar .mce_h4 span.mceText {font-weight:bolder; font-size: 1em}
#text-toolbar .mce_h5 span.mceText {font-weight:bolder; font-size: .83em}
#text-toolbar .mce_h6 span.mceText {font-weight:bolder; font-size: .75em}

