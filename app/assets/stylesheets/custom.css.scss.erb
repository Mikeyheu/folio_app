html,body {
	font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
	font-size:14px;
	color:#333;
	height:100%;
	width:100%;
}

a {
	text-decoration: none;
}

li {
	list-style: none;
}

a.navbutton {
	display:block;
	height:36px;
	width:36px;
}

.left {
	float:left;
}

.right {
	float:right;
}

.primary-navbar {
	height:40px;
	background-color: #1b1b1b;
	background-image: -moz-linear-gradient(top, #222222, #111111);
	background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#222222), to(#111111));
	background-image: -webkit-linear-gradient(top, #222222, #111111);
	background-image: -o-linear-gradient(top, #222222, #111111);
	background-image: linear-gradient(to bottom, #222222, #111111);
	background-repeat: repeat-x;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff222222', endColorstr='#ff111111', GradientType=0);

}

.secondary-navbar {
	height:36px;
	background-color: #0099ff;
	background-image: -moz-linear-gradient(top, #0099ff, #0077dd);
	background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#0099ff), to(#0077dd));
	background-image: -webkit-linear-gradient(top, #0099ff, #0077dd);
	background-image: -o-linear-gradient(top, #0099ff, #0077dd);
	background-image: linear-gradient(to bottom, #0099ff, #0077dd);
	background-repeat: repeat-x;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff0099ff', endColorstr='#ff0077dd', GradientType=0);
}

.tertiary-navbar {
	/*bottom:0;
	position:absolute;*/
	height:36px;
	background-color: #0099ff;
	background-image: -moz-linear-gradient(top, #555555, #444444);
	background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#555555), to(#444444));
	background-image: -webkit-linear-gradient(top, #555555, #444444);
	background-image: -o-linear-gradient(top, #555555, #444444);
	background-image: linear-gradient(to bottom, #555555, #444444);
	background-repeat: repeat-x;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff555555', endColorstr='#ff444444', GradientType=0);
}

.toolbar {
	/*bottom:0;
	position:absolute;*/
	height:36px;
	background-color: #0099ff;
	background-image: -moz-linear-gradient(top, #555555, #444444);
	background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#555555), to(#444444));
	background-image: -webkit-linear-gradient(top, #555555, #444444);
	background-image: -o-linear-gradient(top, #555555, #444444);
	background-image: linear-gradient(to bottom, #555555, #444444);
	background-repeat: repeat-x;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff555555', endColorstr='#ff444444', GradientType=0);
}

.logo {
	margin-top:8px;
	margin-left:10px;
	margin-right:44px;
	font-size:20px;
	font-weight:bold;
	color:white;
	float:left;
}

.nav {
	list-style: none;
	position: relative;
	display: block;
	float:left;
}

.nav-right {
	float: right;
}

.nav-button {
	width:36px;
	height:36px;
}

.nav-button-text {
	width:109px;
	height:36px;
}

.icontext {
	font-weight:normal;
	font-size:14px;
	margin-top:11px;
	margin-left:20px;
	margin-right:5px;
	color:white;
}

.primary-nav > li {
	height:40px;
	margin-left:10px;
	margin-right:10px;
	float:left;
}

.active {
	background-image:url(<%= asset_path 'caret.png' %>);
	background-position: center bottom;
	background-repeat: no-repeat;
}

.primary-nav > li > a {
	color:#FFF;
	font-size:14px;
	font-weight:normal;
	text-decoration: none;
	line-height: 40px;
}


.nav > li {
	color:white;
	/*	font-size:12px;*/
	font-weight:normal;
	float:left;
}

.nav-text {
	margin-top:6px;
	margin-left:10px;
	margin-right:10px;
	color:white;
	font-size:12px;
	font-weight:normal;
	float:left;
}

.content {
	position:absolute;
	top:40px;
	left:0;
	height:300px;
	width:100%;
	background-color:#FFF;
}

.left-panel {
	position:absolute;
	left:0;
	width:220px;
	height:100%;
	background-color:#EEE;
}

.right-panel {
	position:absolute;
	left:220px;
	height:100%;
	background-color:#C4C4C4;
}

.right-inner {
	position:relative;
	overflow:scroll;
	height:100%;
}

.divider-vertical {
	height: 36px;
	border-right: 1px solid #5bbdff;
}

.divider-gray {
	border-right: 1px solid #999;
}

.icon {
	position:relative;
	top:10px;
	left:10px;
	height:16px;
	width:16px;
	background-image: url(<%= asset_path 'nav-icons.png' %>); /*your location of the image may differ*/
}





.settings {background-position:0px 0px;}
.view {background-position:0px -16px;}
.save {background-position:0px -32px;}
.trash {background-position:0px -48px;}
.upload {background-position:0px -64px;}
.add {background-position:0px -80px;}
.caret-left {background-position:0px -96px;}
.caret-right {background-position:0px -112px;}
.stack {background-position:0px -128px;}

.content-page {background-position:-16px 0px;}
.folder {background-position:-16px -16px;}
.link {background-position:-16px -32px;}
.gallery {background-position:-16px -48px;}


#add-element:hover > .icon {background-position:-16px -80px;}
#settings:hover > .icon {background-position:-16px 0px;}
#view:hover > .icon {background-position:-16px -16px;}
#save:hover > .icon {background-position:-16px -32px;}
#trash:hover > .icon {background-position:-16px -48px;}

#text-object {
	position:absolute;
	width:600px;
	padding:20px;
	line-height: 1.5em;
}

.hide {
	display: none;
	height:0;
}


#page-menu {
	height:400px;
	background-color:#EEE;
	/*overflow:scroll;*/
}

#page-menu div:hover, #archive-menu div:hover, #page-menu div.active, #archive-menu div.active, .menu-hover {
	color:#FFF;
	background-color: #0099ff;
	background-image: -moz-linear-gradient(top, #0099ff, #0077dd);
	background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#0099ff), to(#0077dd));
	background-image: -webkit-linear-gradient(top, #0099ff, #0077dd);
	background-image: -o-linear-gradient(top, #0099ff, #0077dd);
	background-image: linear-gradient(to bottom, #0099ff, #0077dd);
	background-repeat: repeat-x;
	filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ff0099ff', endColorstr='#ff0077dd', GradientType=0);
}

#page-menu div:hover .content-page, #archive-menu div:hover .content-page, .menu-hover .content-page {background-position:0px 0px;}
#page-menu div:hover .folder, #archive-menu div:hover .folder, .menu-hover .folder {background-position:0px -16px;}
#page-menu div:hover .link, #archive-menu div:hover .link, .menu-hover .link {background-position:0px -32px;}
#page-menu div:hover .gallery, #archive-menu div:hover .gallery,  .menu-hover .gallery {background-position:0px -48px;}

.folder.open {background-position:-16px -64px;}
#page-menu div:hover .folder.open, #archive-menu div:hover .folder.open, .menu-hover .folder.open {background-position:0px -64px;}

#panes a  {
	color:#333;
}

#panes div:hover > a {
	color:#FFF;
}

.menu-title {
	display: block;
	padding: 7px 0px 5px 12px;
	font-size: 11px;
	font-weight: bold;
	line-height: 20px;
	color: #999;
	text-transform: uppercase;
}

#page-menu div.menu-title:hover, #archive-menu div.menu-title:hover {
	color:#999;
	background-color: #EEE;
	background-image: none;
}

